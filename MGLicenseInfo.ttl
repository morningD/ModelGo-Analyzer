@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mg: <http://www.modelgo.li/rdf/terms#> . # a placeholder

###################    Apache-2.0    ###################
mg:Apache-2.0 a mg:LicenseInfo ;
    mg:licenseId "Apache-2.0" ;
    mg:licenseName "Apache License 2.0" ;
    mg:targetWorkType mg:dataset ;
    mg:licenseVersion 2.0 ;
    mg:isFsfLibre true ;
    mg:isOsiApproved true ;
    mg:isPermissive true ;
    # mg:isOpenDomain false ; # No existing means false here.
    # mg:isCopyleft false ;
    
    mg:grant [
        a mg:Right ;
        mg:isPatent true ;
        mg:isCopyright true ;
        mg:hasRightPrefix mg:perpetual, mg:worldwide, mg:non-exclusive, mg:no-charge, mg:royalty-free, mg:irrevocable ;
        mg:hasUsage mg:use, mg:modify, mg:merge, mg:redistribute, mg:sublicense, mg:commercial_use
    ] ;
    mg:reserve [ # There is no need to write the prefix and isPatent/isCopyright for reserve Right
        a mg:Right ;
        mg:hasUsage mg:trademark_use, mg:copyright, mg:patent, mg:trademark
    ] ;
    
    mg:hasRule mg:Apache-2.0-duplication-rule, mg:Apache-2.0-independent-rule, mg:Apache-2.0-nodefinition-rule ;
    mg:hasRule mg:Apache-2.0-duplication-fuzz-rule, mg:Apache-2.0-independent-fuzz-rule, mg:Apache-2.0-nodefinition-fuzz-rule ; # Fuzz rules
    
    mg:licenseUrl <https://www.apache.org/licenses/LICENSE-2.0> . # We place the URL link at the tail to make it more robust to comment.


###################    MIT    ###################
mg:MIT a mg:LicenseInfo ;
    mg:licenseId "MIT" ;
    mg:licenseName "MIT License" ;
    mg:targetWorkType mg:software ;
    mg:licenseVersion "NaN"^^xsd:float ;
    mg:isFsfLibre true ;
    mg:isOsiApproved true ;
    mg:isPermissive true ;
    # mg:isOpenDomain false ;
    # mg:isCopyleft false ;
    
    mg:grant [
        a mg:Right ;
        mg:isCopyright true ;
        mg:hasRightPrefix mg:no-charge, mg:mg:any-person ;
        mg:hasUsage mg:use, mg:modify, mg:merge, mg:redistribute, mg:sublicense, mg:commercial_use
    ] ; # No rights are reserved under the MIT License; however, generally speaking, patent, trademark, and copyright rights are implicitly reserved.
    
    mg:hasRule mg:MIT-duplication-rule, mg:MIT-independent-rule, mg:MIT-nodefinition-rule ;
    mg:hasRule mg:MIT-duplication-fuzz-rule, mg:MIT-independent-fuzz-rule, mg:MIT-nodefinition-fuzz-rule ; # Fuzz rules
    
    mg:licenseUrl <https://opensource.org/license/mit/> .


###################    Unlicense    ###################
# We treat the Unlicense as the default choice for any intermediate work that imposes no restrictions and limitations on its covered work.
mg:Unlicense a mg:LicenseInfo ; 
    mg:licenseId "Unlicense" ;
    mg:licenseName "The Unlicense" ;
    #mg:targetWorkType mg:software ; # We ASSUME it applies to any type of work, even though it was originally designed as a software license.
    mg:licenseVersion "NaN"^^xsd:float ;
    mg:isFsfLibre true ;
    mg:isOsiApproved true ;
    mg:isPermissive true ;
    mg:isOpenDomain true ;
    # mg:isCopyleft false ;
    
    mg:grant [
        a mg:Right ;
        mg:isCopyright true ;
        mg:hasRightPrefix mg:any-person, mg:irrevocable ; # We consider it is irrevocable due to it is a dedication license.
        mg:hasUsage mg:use, mg:modify, mg:merge, mg:redistribute, mg:sublicense, mg:commercial_use, mg:copyright # Unlicense also grants you the copyright of work
    ] ;
    
    mg:hasRule mg:Unlicense-independent-rule ;
    
    mg:licenseUrl <https://opensource.org/license/unlicense/> .


