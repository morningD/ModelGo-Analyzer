@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mg: <http://www.modelgo.li/rdf/terms#> . # a placeholder

# Apache-2.0 Rules
mg:Apache-2.0-duplication-rule a mg:Rule ;
    mg:hasOutputDef mg:duplication ;
    mg:targetAction mg:Copy ;
    mg:targetInputWorkForm mg:code, mg:exe ;
    mg:targetOutputWorkForm mg:code, mg:exe ;
    mg:relicense mg:none-license ; # It means the output work cannot be relicensed to another license.
    mg:allowSharing true . # It means the output work can be shared with third parties.
    
mg:Apache-2.0-duplication-fuzz-rule a mg:Rule ; # Fuzz matching of input and output work forms (according to our purposed taxonomy)
    mg:hasOutputDef mg:duplication ;
    mg:targetAction mg:Copy ;
    mg:targetInputWorkForm mg:linguistics, mg:vision, mg:weights, mg:raw-form, mg:binary-form ; # Fuzz match to raw forms and binary forms of dataset and models.
    mg:targetOutputWorkForm mg:linguistics, mg:vision, mg:weights, mg:raw-form, mg:binary-form ;
    mg:relicense mg:none-license ; 
    mg:allowSharing true .

mg:Apache-2.0-independent-rule a mg:Rule ;
    mg:hasOutputDef mg:independent ;
    mg:targetAction mg:Combine ;
    mg:targetInputWorkForm mg:code, mg:exe ;
    mg:targetOutputWorkForm mg:code, mg:exe ;
    mg:relicense mg:any-license ;
    mg:allowSharing true .

mg:Apache-2.0-independent-fuzz-rule a mg:Rule ;
    mg:hasOutputDef mg:independent ;
    mg:targetAction mg:Combine ;
    mg:targetInputWorkForm mg:linguistics, mg:vision, mg:weights, mg:raw-form, mg:binary-form ;
    mg:targetOutputWorkForm mg:linguistics, mg:vision, mg:weights, mg:raw-form, mg:binary-form ;
    mg:relicense mg:any-license ;
    mg:allowSharing true .

# MIT Rules
mg:MIT-independent-rule a mg:Rule ;
    mg:hasOutputDef mg:independent ;
    mg:targetAction mg:Combine ;
    mg:targetInputWorkForm mg:code, mg:exe ;
    mg:targetOutputWorkForm mg:code, mg:exe ;
    mg:relicense mg:any-license ;
    mg:allowSharing true .

mg:MIT-independent-fuzz-rule a mg:Rule ;
    mg:hasOutputDef mg:independent ;
    mg:targetAction mg:Combine ;
    mg:targetInputWorkForm mg:linguistics, mg:vision, mg:weights, mg:raw-form, mg:binary-form ;
    mg:targetOutputWorkForm mg:linguistics, mg:vision, mg:weights, mg:raw-form, mg:binary-form ;
    mg:relicense mg:any-license ;
    mg:allowSharing true .
    