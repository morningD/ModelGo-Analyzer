{ # Mixwork
    ?work a mg:Work .
    ?work mg:workName ?wname .
    ?work mg:hasMixwork ?mixwork .
    ?mixwork mg:workName ?mname .
} log:query {
    ?work a mg:Work .
    ?work mg:workName ?wname .
    ?work mg:hasMixwork ?mixwork .
    ?mixwork mg:workName ?mname .
} .

