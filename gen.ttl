@prefix : <http://example.org/ns#> .
@prefix mg: <http://www.modelgo.li/rdf/terms#> .

:reg1 a mg:RegisterLicense ;
    mg:actionId "reg1" ;
    mg:hasInput [ mg:targetLicense mg:CC-BY-SA-4.0 ;
            mg:targetWork :Wikipedia ] ;
    mg:yieldAuxWork :train6 .

:reg3 a mg:RegisterLicense ;
    mg:actionId "reg3" ;
    mg:hasInput [ mg:targetLicense mg:MIT ;
            mg:targetWork :Whisper ] ;
    mg:yieldOutputWork :train6 .

:reg4 a mg:RegisterLicense ;
    mg:actionId "reg4" ;
    mg:hasInput [ mg:targetLicense mg:Apache-2.0 ;
            mg:targetWork :BERT ] ;
    mg:yieldOutputWork :combine7 .

:reg5 a mg:RegisterLicense ;
    mg:actionId "reg5" ;
    mg:hasInput [ mg:targetLicense mg:OpenRAIL-M ;
            mg:targetWork :BLOOM ] ;
    mg:yieldOutputWork :combine7 .

:BERT a mg:work ;
    mg:hasWorkForm mg:weights ;
    mg:hasWorkType mg:model ;
    mg:workName "BERT" .

:BLOOM a mg:work ;
    mg:hasWorkForm mg:weights ;
    mg:hasWorkType mg:model ;
    mg:workName "BLOOM" .

:Whisper a mg:work ;
    mg:hasWorkForm mg:weights ;
    mg:hasWorkType mg:model ;
    mg:workName "Whisper" .

:Wikipedia a mg:work ;
    mg:hasWorkForm mg:literary ;
    mg:hasWorkType mg:dataset ;
    mg:workName "Wikipedia" .

:combine7 a mg:Combine ;
    mg:actionId "combine7" ;
    mg:hasInput [ ] ;
    mg:hasOutput [ ] ;
    mg:yieldOutputWork :combine8 .

:combine8 a mg:Combine ;
    mg:actionId "combine8" ;
    mg:hasInput [ ] ;
    mg:hasOutput [ ] .

:train6 a mg:Train ;
    mg:actionId "train6" ;
    mg:hasInput [ ] ;
    mg:hasOutput [ ] ;
    mg:yieldOutputWork :combine8 .

