@prefix : <http://example.org/ns#> .
@prefix mg: <http://www.modelgo.li/rdf/terms#> .

:reg1 a mg:RegisterLicense ;
    mg:actionId "reg1" ;
    mg:hasInput [ mg:targetLicense mg:MIT ;
            mg:targetWork :Whisper ] ;
    mg:yieldOutputWork :combine3 .

:reg2 a mg:RegisterLicense ;
    mg:actionId "reg2" ;
    mg:hasInput [ mg:targetLicense mg:Apache-2.0 ;
            mg:targetWork :DETR ] ;
    mg:yieldOutputWork :combine3 .

:DETR a mg:work ;
    mg:hasWorkForm mg:code ;
    mg:hasWorkType mg:model ;
    mg:workName "DETR" .

:Whisper a mg:work ;
    mg:hasWorkForm mg:code ;
    mg:hasWorkType mg:model ;
    mg:workName "Whisper" .

:combine3 a mg:Combine ;
    mg:actionId "combine3" ;
    mg:hasInput [ ] ;
    mg:hasOutput [ ] .

