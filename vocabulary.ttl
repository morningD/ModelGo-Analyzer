@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix mg: <http://www.modelgo.li/rdf/terms#> . # a placeholder

# Note: It is not an urgent need to link our vocabulary to SPDX; we can align it in the future.
#mg:LicenseInfo rdfs:subClassOf spdx:SimpleLicensingInfo .


# General vocabulary

mg:hasItems a rdfs:Property ;
    rdfs:range rdf:List .

mg:is a rdfs:Property ;
    rdfs:comment "The value of a instance." .

mg:broader a rdfs:Property ;
    rdfs:comment "This property is used to point to an instance with a broader meaning." .

# Vocabulary of Action

mg:Action a rdfs:Class ;
    rdfs:label "Action"@en ;
    rdfs:comment "The superset of all actions (e.g., registering a license, reusing, publishing) in ModelGo." .

mg:yieldOutputWork a rdfs:Property ;
    rdfs:domain mg:Action ;
    rdfs:range mg:Action .

mg:targetAction a rdfs:Property ;
    rdfs:range mg:Action ;
    rdfs:comment " The target Action of a request/ruling/report".

mg:actionId a rdfs:Property ;
    rdfs:domain mg:Action ;
    rdfs:range xsd:string ;
    rdfs:comment "The display name of this action." .

mg:hasInput a rdfs:Property ;
    rdfs:domain mg:Action ;
    rdfs:comment "The input of an action." .

mg:hasOutput a rdfs:Property ;
    rdfs:domain mg:Action ;
    rdfs:comment "The output of an action." .

mg:targetLicense a rdfs:Property ; # The target license for register
    rdfs:range mg:LicenseInfo ;
    rdfs:label "targetLicense"@en ;
    rdfs:comment "Property which will build a link from input/output (blank node) to license info." .

mg:targetWork a rdfs:Property ;
    rdfs:range mg:Work ;
    rdfs:commnet "The traget work of an input/output or request/ruling." .

mg:targetSubwork a rdfs:Property ;
    rdfs:range mg:Work ;
    rdfs:commnet "The traget subwork of an input." .

mg:targetAuxwork a rdfs:Property ;
    rdfs:range mg:Work ;
    rdfs:commnet "The traget auxwork of an input." .

mg:RegisterLicense a rdfs:Class ;
    rdfs:subClassOf mg:Action ;
    rdfs:lable "RegisterLicense"@en ;
    rdfs:comment "This action attempts to register a license for the works." .

mg:Combine a rdfs:Class ;
    rdfs:subClassOf mg:Action ;
    rdfs:lable "Combine"@en ;
    rdfs:comment "This action attempts to bundle multiple works to construct a new work." .

mg:Copy a rdfs:Class ;
    rdfs:subClassOf mg:Action ;
    rdfs:lable "Copy"@en ;
    rdfs:comment "This action attempts to copy a work, there has a need to check the right of making ducplication." .

# Vocabulary of Work

## Definitions of work
mg:Work a rdfs:Class ;
    rdfs:label "Work"@en ;
    rdfs:comment "A class representing the licenseable work, it has one type and one form." .

mg:workName a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of this work, such as BLOOM, Imagenet." .

mg:hasWorkType a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range mg:WorkType .

mg:hasWorkForm a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range mg:WorkForm .

mg:hasLicense a rdfs:Property ; # Registered license of a work.
    rdfs:domain mg:Work ;
    rdfs:range mg:LicenseInfo .

mg:hasProvenance a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Work .

mg:hasReliedwork a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Work .

mg:hasMixwork rdfs:subPropertyOf mg:hasReliedwork ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Work .

mg:hasSubwork rdfs:subPropertyOf mg:hasReliedwork ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Work .

mg:hasAuxwork rdfs:subPropertyOf mg:hasReliedwork ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Work .

## Type of work
mg:WorkType a rdfs:Class ;
    rdfs:label "WorkType"@en ;
    rdfs:comment "A class representing the type of a licensed work such as software, dataset, or model." .

mg:targetWorkType a rdfs:Property ;
    rdfs:range mg:WorkType .

# Known work types (instances)
mg:software a mg:WorkType ;
    rdfs:label "software"@en .

mg:dataset a mg:WorkType ;
    rdfs:label "dataset"@en .

mg:model a mg:WorkType ;
    rdfs:label "model"@en .

mg:mixed-type a mg:WorkType ;
    rdfs:label "mixed-type"@en .

## Form of work
mg:WorkForm a rdfs:Class ;
    rdfs:label "WorkForm"@en ;
    rdfs:comment "A class representing the form of a licensed work such as code, binary, service." .

mg:formName a rdfs:Property ;
    rdfs:domain mg:WorkForm ;
    rdfs:range xsd:string .

mg:hasWorkForm a rdfs:Property ;
    rdfs:range mg:WorkForm .

## Known work forms (instances)
mg:Raw rdfs:subClassOf mg:WorkForm .
mg:Binary rdfs:subClassOf mg:WorkForm .
mg:Service rdfs:subClassOf mg:WorkForm .

mg:code a mg:Raw ;
    rdfs:formName "code" ; 
    mg:targetWorkType mg:software ;
    mg:broader mg:raw-form ;
    rdfs:comment "Software in the form of source code." .
mg:linguistics a mg:Raw ;
    rdfs:formName "linguistics" ; 
    mg:targetWorkType mg:dataset ;
    mg:broader mg:raw-form ;
    rdfs:comment "Dataset in the form of linguistics resource." .
mg:vision a mg:Raw ;
    rdfs:formName "vision" ; 
    mg:targetWorkType mg:dataset ;
    mg:broader mg:raw-form ;
    rdfs:comment "Dataset in the form of vision resource." . 
mg:weights a mg:Raw ;
    rdfs:formName "weights" ; 
    mg:targetWorkType mg:model ;
    mg:broader mg:raw-form ;
    rdfs:comment "ML model in the form of model weights (parameters)." .
mg:raw-form a mg:Raw ;
    rdfs:formName "raw-form" ; 
    #mg:targetWorkType mg:software, mg:dataset, mg:model, mg:mixed-type ; #NOTE: General work form that does not have a specific target work type.
    mg:broader mg:raw-form ;
    rdfs:comment "General raw form for unspecified resource." .

mg:exe a mg:Binary ;
    rdfs:formName "exe" ;
    mg:targetWorkType mg:software ;
    mg:broader mg:binary-form ;
    rdfs:comment "Software as a compiled program." .
mg:binary-form a mg:Binary ;
    rdfs:formName "binary-form" ;
    #mg:targetWorkType mg:softwar, mg:dataset, mg:model, mg:mixed-type ;
    mg:broader mg:binary-form ;
    rdfs:comment "General binary form for unspecified resource." .

mg:saas a mg:Service ;
    rdfs:formName "saas" ;
    mg:targetWorkType mg:software ;
    mg:broader mg:service-form ;
    rdfs:comment "Software as a SaaS." .
mg:service-form a mg:Service ;
    rdfs:formName "service-form" ;
    #mg:targetWorkType mg:software, mg:dataset, mg:model, mg:mixed-type ;
    mg:broader mg:service-form ;
    rdfs:comment "General service form for unspecified resource." .

mg:mixed-form a mg:WorkForm ;
    rdfs:formName "mixed-form" ;
    mg:broader mg:mixed-form ;
    rdfs:comment "Mixed form for a collection of resources in different forms." .

# Vocabulary of Licenses
mg:LicenseInfo a rdfs:Class ;
    rdfs:label "LicenseInfo"@en ;
    rdfs:comment "This class describes the basic information about a license (software, dataset, ML model)." .

mg:isFsfLibre a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:boolean .

mg:isOsiApproved a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:boolean .

mg:isCopyleft a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:boolean .

mg:isOpenDomain a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:boolean .

mg:isPermissive a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:boolean .

mg:licenseId a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:string ;
    rdfs:comment "The licenseId is the SPDX short identifier for the license." .

mg:licenseId a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:string .

mg:licenseName a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:string .

mg:licenseVersion a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:float .

mg:licenseUrl a rdfs:Property ;
    rdfs:domain mg:LicenseInfo ;
    rdfs:range xsd:anyURI .

mg:Right a rdfs:Class ;
    rdfs:lable "Right"@en ;
    rdfs:comment "This class describes the granted/reserve rights according to license terms" .

mg:isPatent a rdfs:Property ;
    rdfs:domain mg:Right ;
    rdfs:range xsd:boolean .

mg:isCopyright a rdfs:Property ;
    rdfs:domain mg:Right ;
    rdfs:range xsd:boolean .

mg:grant a rdfs:Property ;
    #rdfs:domain mg:LicenseInfo ; #rdfs:domain mg:Request
    rdfs:range mg:Right .

mg:reserve a rdfs:Property ;
    #rdfs:domain mg:LicenseInfo ; #rdfs:domain mg:Request
    rdfs:range mg:Right .

mg:RightPrefix a rdfs:Class ;
    rdfs:comment "The prefix (condition) of granted rights, i.e., irrevocable, worldwide, non-exclusive." .

mg:hasRightPrefix a rdfs:Property ;
    rdfs:domain mg:Right ;
    rdfs:range mg:RightPrefix .

mg:irrevocable a mg:RightPrefix .
mg:revocable a mg:RightPrefix .
mg:worldwide a mg:RightPrefix .
mg:non-exclusive a mg:RightPrefix .
mg:exclusive a mg:RightPrefix .
mg:royalty-free a mg:RightPrefix .
mg:no-charge a mg:RightPrefix .
mg:perpetual a mg:RightPrefix .
mg:any-person a mg:RightPrefix .
mg:sublicensable a mg:RightPrefix .

mg:Usage a rdfs:Class ;
    rdfs:comment "The allow/not allow usages according the granting terms, i.e., use, redistribute, modify." .

mg:hasUsage a rdfs:Property ;
    rdfs:domain mg:Right ;
    rdfs:range mg:Usage .

mg:use a mg:Usage .
mg:modify a mg:Usage .
mg:redistribute a mg:Usage .
mg:merge a mg:Usage .
mg:redistribute a mg:Usage .
mg:commercial_use a mg:Usage .
mg:patent a mg:Usage .
mg:trademark a mg:Usage .
mg:trademark_use a mg:Usage .
#mg:patent_use a mg:Usage . # Equal to mg:isPatent.
#mg:copy a mg:Usage . # A license does not grant the right to copy is meaningless.

# Vocabulary of License Rule
mg:LicenseRule a rdfs:Class ;
    rdfs:label "LicenseRule"@en ;
    rdfs:comment "This class describes the rules for output definitions (e.g., derivate, modification) corresponding to the action." .

# Be used to record the rule of input work's license and applied rule of output work.
mg:hasRule a rdfs:Property ;
    rdfs:range mg:LicenseRule .

mg:targetInputWorkForm a rdfs:Property ;
    rdfs:domain mg:LicenseRule ;
    rdfs:range mg:WorkForm .

mg:targetOutputWorkForm a rdfs:Property ;
    rdfs:domain mg:LicenseRule ;
    rdfs:range mg:WorkForm .

mg:targetActionType a rdfs:Property ;
    rdfs:domain mg:LicenseRule ;
    rdfs:range rdfs:Class .

mg:allowSharing a rdfs:Property ;
    rdfs:domain mg:LicenseRule ;
    rdfs:range xsd:boolean .

mg:relicense a rdfs:Property ;
    rdfs:domain mg:LicenseRule ;
    rdfs:range mg:LicenseCategory .

mg:LicenseCategory a rdfs:Class ;
    rdfs:comment "This class is used to classify the category of licenses." .

mg:any-license a mg:LicenseCategory .
mg:none-license a mg:LicenseCategory .
mg:compatible-license a mg:LicenseCategory . # It is a concept which depends on practical license.

mg:Restriction a rdfs:Class ;
    rdfs:comment "This class is used to describe the restrictions that should be applied to a LicenseRule." .

mg:hasRestriction a rdfs:Property ;
    rdfs:range mg:Restriction .

mg:state_changes_restriction a mg:Restriction ;
    mg:content "This work must state changes according to related license(s)." .

# Vocabulary for requesting rights of a input work raised by an action during the ML reuse process.
mg:Request a rdfs:Class ;
    rdfs:comment "The need to grant or reserve rights for a input work depends on its method of reuse." .

mg:hasRequest a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Request .

mg:Ruling a rdfs:Class ;
    rdfs:comment "Record the definitions of a output work (e.g., derivatives, modification, duplication) according the rules of licenses of input works." .

mg:hasRuling a rdfs:Property ;
    rdfs:domain mg:Work ;
    rdfs:range mg:Ruling .

mg:OutputDef a rdfs:Class ; 
    rdfs:comment "The definition of output work, it should be understood in the context of the specific license used in each input work." .

mg:hasOutputDef a rdfs:Property ;
    rdfs:domain mg:Ruling ;
    rdfs:range mg:OutputDef .

mg:duplication a mg:OutputDef .
mg:modification a mg:OutputDef .
mg:independent a mg:OutputDef .
mg:derivative a mg:OutputDef .
mg:nodefinition a mg:OutputDef .

# Vocabulary of Report (notice, restriction, warning, error)
mg:Report a rdfs:Class ;
    rdfs:label "Report"@en ;
    rdfs:comment "The superset of all reports (e.g., notice, warning, error) in ModelGo." .

mg:Notice rdfs:subClassOf mg:Report ;
    rdfs:comment "Notices like [Revocable license], [No public license]." . # See the Table1 in ModelGo paper (WWW'24) for more info.

mg:Restriction rdfs:subClassOf mg:Report ;
    rdfs:comment "Restrictions like [State changes], [Responsible AI use]." .

mg:Warning rdfs:subClassOf mg:Report ;
    rdfs:comment "Warnings like [Disclose self], [Rights not explicitly granted]." .

mg:Error rdfs:subClassOf mg:Report ;
    rdfs:comment "Errors like [Cannot relicense], [Rights not granted], [Cannot share]." .

mg:reportBy a rdfs:Property ;
    rdfs:domain mg:Report .
    #rdfs:range mg:Action ; #TBD: Mayby we can let it free and enable reportBy->Work  or reportBy -> [work, action]

mg:reportType a rdfs:Property ;
    rdfs:domain mg:Report ;
    rdfs:range xsd:string ;
    rdfs:comment "The predefined type of a report, such as [License Type Mismatch] [Revocable License] [No Public License]." .

mg:content a rdfs:Property ;
    rdfs:range xsd:string .


